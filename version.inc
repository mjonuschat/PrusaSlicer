# Included by CMakeLists, edited by the build script
# (the version numbers are generated by the build script from the git current label)

set(SLIC3R_APP_NAME "PrusaSlicer")
set(SLIC3R_APP_KEY "PrusaSlicer")
set(SLIC3R_VERSION "2.9.3-rc1")
set(SLIC3R_BUILD_ID "PrusaSlicer-${SLIC3R_VERSION}+BOSS")
set(SLIC3R_RC_VERSION "2,9,3,0")
set(SLIC3R_RC_VERSION_DOTS "2.9.3.0")

string(REPLACE "," ";" VERSION_LIST ${SLIC3R_RC_VERSION})
list(GET VERSION_LIST 0 SLIC3R_VERSION_MAJOR)
list(GET VERSION_LIST 1 SLIC3R_VERSION_MINOR)
list(GET VERSION_LIST 2 SLIC3R_VERSION_PATCH)
